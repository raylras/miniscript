{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ESNext"],
    "rootDir": "src",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "strict": true,
    "outDir": "out",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "out",
    "node_modules"
  ]
}
